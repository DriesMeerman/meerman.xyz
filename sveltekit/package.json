{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
    "predev": "node ../generateBlogIndex.js && node ../convertMarkdown.js && rsync -a --delete ../static/ ./static/",
    "dev": "vite dev",
    "prebuild": "node ../generateBlogIndex.js && node ../convertMarkdown.js && rsync -a --delete ../static/ ./static/",
    "build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"particles.js": "github:DriesMeerman/particles.js"
	}
}
